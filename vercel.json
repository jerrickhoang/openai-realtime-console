{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/debug",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "buildCommand": "node vercel-build.js",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "outputDirectory": "client/client/dist"
} 